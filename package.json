{
  "name": "impact-app",
  "version": "0.24.3",
  "description": "Reactive State Management for React",
  "author": "Christian Alfoni <christianalfoni@gmail.com>",
  "license": "MIT",
  "repository": "git+https://github.com/christianalfoni/impact.git",
  "module": "./dist/esm/index.js",
  "main": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p ./tsconfig.build-cjs.json",
    "build:esm": "tsc -p ./tsconfig.build-esm.json",
    "clean": "rimraf ./dist",
    "typecheck": "tsc --noEmit",
    "prepublish": "yarn build"
  },
  "keywords": [
    "typescript",
    "react",
    "imperative",
    "async"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "immer": "^10.0.2",
    "react": "*",
    "stackframe": "^1.3.4",
    "stacktrace-gps": "^3.1.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@babel/plugin-proposal-explicit-resource-management": "^7.22.6",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@radix-ui/themes": "^1.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-react": "^4.0.4",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.3",
    "react-dom": "^18.2.0",
    "rimraf": "^2.6.1",
    "ts-jest": "^29.0.5",
    "tslib": "^2.6.2",
    "typed-client-router": "^0.6.0",
    "typescript": "^5.2.1-rc",
    "vite": "^4.4.0",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
