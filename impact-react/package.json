{
  "name": "impact-react",
  "version": "1.3.2",
  "description": "Reactive state management for React",
  "author": "Christian Alfoni <christianalfoni@gmail.com>",
  "license": "MIT",
  "type": "module",
  "repository": "git+https://github.com/christianalfoni/impact.git",
  "module": "./dist/esm/index.js",
  "main": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "dev": "tsc -p ./tsconfig.build-esm.json --watch",
    "build": "yarn clean && yarn build:cjs && yarn build:esm && bash ./fixModuleTypes",
    "build:cjs": "tsc -p ./tsconfig.build-cjs.json && echo \"'use strict';\nexports.isDevelopment = typeof process === 'undefined' || process.env.NODE_ENV === 'development'\" > ./dist/cjs/env.js",
    "build:esm": "tsc -p ./tsconfig.build-esm.json && echo \"export const isDevelopment = import.meta && import.meta?.env?.DEV\" > ./dist/esm/env.js",
    "clean": "rimraf ./dist",
    "typecheck": "tsc --noEmit",
    "prepublish": "yarn build"
  },
  "keywords": [
    "typescript",
    "react",
    "context",
    "reactive",
    "signals",
    "state",
    "global"
  ],
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {
    "decamelize": "^6.0.0",
    "preact": "^10.19.3",
    "stackframe": "^1.3.4",
    "stacktrace-gps": "^3.1.2"
  }
}
