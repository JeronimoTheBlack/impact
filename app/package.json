{
  "name": "impact-app",
  "version": "0.38.0",
  "description": "Reactive applications for React",
  "author": "Christian Alfoni <christianalfoni@gmail.com>",
  "license": "MIT",
  "repository": "git+https://github.com/christianalfoni/impact.git",
  "module": "./dist/esm/context.js",
  "main": "./dist/esm/context.js",
  "exports": {
    ".": {
      "import": "./dist/esm/context.js",
      "types": "./dist/esm/context.d.ts"
    }
  },
  "types": "./dist/esm/context.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p ./tsconfig.build-cjs.json",
    "build:esm": "tsc -p ./tsconfig.build-esm.json",
    "clean": "rimraf ./dist",
    "typecheck": "tsc --noEmit",
    "prepublish": "yarn build"
  },
  "keywords": [
    "typescript",
    "react",
    "context",
    "reacive"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "impact-signal": "latest",
    "impact-context": "latest"
  },
  "peerDependencies": {
    "react": "*"
  }
}
